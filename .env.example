# The port your Express server will listen on
PORT=3000

# MongoDB connection string (replace <username>, <password>, <cluster-url>, <database>)
CONNECTION_STRING=mongodb+srv://<username>:<password>@<cluster-url>/<database>?retryWrites=true&w=majority&appName=<app-name>

# Secret key for signing JWT access tokens
ACCESS_TOKEN_SECRET=your_access_token_secret_here